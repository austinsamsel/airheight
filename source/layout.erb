<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <title>High Tops - Design, Development, and Consulting<%= ' - ' + current_article.title unless current_article.nil? %></title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link rel="canonical" href="http://hightopsnyc.com" />
    <%= auto_display_meta_tags %>
    <%= partial "partials/favicon" %>

    <%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>
    <%= stylesheet_link_tag "all" %>
     <link href='//fonts.googleapis.com/css?family=Archivo+Black|Archivo+Narrow:400,700,400italic|Enriqueta:400,700' rel='stylesheet' type='text/css'>
  </head>
  <body>

  <%= yield %>

  <%= javascript_include_tag  "home" %>
  <%= google_analytics_tag %>
  </body>
</html>
